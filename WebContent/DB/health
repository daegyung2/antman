create table health(  //질병 및 부위 별 건강 정보 
  hid number primary key not null,  //건강 정보 아이디 시퀀스
  hname varchar2(1000),
  body_part varchar2(500) not null,        // 신체 부위
  decease varchar2(500) not null,          //질병명
  symptoms varchar2(1000),                 //증상
  r_decease varchar2(1000),                // 관련 질병 
  dpname varchar2(500),                  // 관련 진료 과
  same_lang varchar2(4000),                 //질병 이름 동의어 
  himg varchar2(4000)                     //질병 사진
  );