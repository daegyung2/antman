create table appointment(
   Aid varchar2(500) primary key not null,
   id varchar2(200),
   name varchar2(200),
   agentApp number default 0 not null,
   jumin1 number not null,
   jumin2 number not null,
   Did varchar2(500) not null,
   Adate date default sysdate,
   symptom varchar2(4000) not null,
   Odecease varchar2(400),
   Odrug varchar2(4000),
   descripton varchar2(4000),
   reg-number varchar2(500) not null
   ); 