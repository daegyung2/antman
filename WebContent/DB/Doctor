create table doctor(

  DId varchar2(200) primary key not null,
  Dreg_num varchar2(500) not null,
  name varchar2(200),
  major varchar2(4000),
  depart_id varchar2(500),
  degree1 varchar2(4000),
  degree2 varchar2(4000),
  degree3 varchar2(4000),
  degree4 varchar2(4000),
  degree5 varchar2(4000),
  report1 varchar2(4000),
  report2 varchar2(4000),
  report3 varchar2(4000),
  report4 varchar2(4000),
  report5 varchar2(4000),
  
  );