create table employee(  //직원 정보
  Eid number default 10000 primary key not null,  //직원 사번
  name varchar2(200) not null,             //이름
  major varchar2(4000),                    //전공
  dpname varchar2(500)                 //부서이름
  emimg1 varchar2(4000),                   //이미지
  emimg2 varchar2(4000),
  emimg3 varchar2(4000),
  emimg4 varchar2(4000),
  emimg5 varchar2(4000),
  emdescription varchar2(4000)
  );