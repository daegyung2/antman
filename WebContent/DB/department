create table department(
   Depart_id varchar2(200) primary key not null,
   dname varchar2(500) not null,
   initial varchar2(100) not null,
   d_info1 varchar2(4000),
   d_info2 varchar2(4000),
   d_info3 varchar2(4000),
   d_info4 varchar2(4000),
   d_info5 varchar2(4000),
   d_info6 varchar2(4000),
   d_info7 varchar2(4000),
   d_info8 varchar2(4000),
   d_info9 varchar2(4000),
   d_info10 varchar2(4000)
   );