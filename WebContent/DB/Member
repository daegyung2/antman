create table member (
  id varchar2(200) primary key not null, 
  passwd varchar2(200) not null,
  name varchar2(100) not null,
  age number,
  gender varchar2(100),
  phone number not null,
  e-mail varchar2(200),
  getMail varchar2(200),
  Eid varchar2(500),
  Dreg_num varchar2(500),
  reg-number number default 10000
  );