<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC '-//ibatis.apache.org//DTD Sql Map 2.0//EN' 'http://ibatis.apache.org/dtd/sql-map-2.dtd'>
<sqlMap namespace="MyQnA">

<typeAlias alias="MyQnA" type="admin.bean.MyQnADTO"/>

	

<select id="selectMyQnA" parameterClass="MyQnA">
  select id,name,content,drname from MyQnA where id = #id#
</select>


<insert id="insertMyQnA" parameterClass="MyQnA">
		
      		INSERT INTO MyQnA (id,
                         	   name,
                         	   content,
                         	   answer,
                         	   drname,
                         	   q_time,
                         	   a_time
                         )
                	 VALUES (#id#,
                	 		#name#,
                         	#content#,
                         	#answer#,
                         	#drname#,
                         	#q_time#,
                         	#a_time#
                          )
		</insert>
</sqlMap>
